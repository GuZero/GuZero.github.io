/* pages/depositOrder/index.wxss */

page {
  background-color: #efeff4;
  overflow-x: hidden;
  -webkit-overflow-scrolling: touch;
}

.depositOrder {
  overflow-x: hidden;
}

.header {
  height: 90rpx;
  line-height: 90rpx;
  padding: 0 32rpx;
  font-size: 32rpx;
  color: #c3c3c3;
  border-bottom: 1rpx #eee solid;
  background-color: #4285f4;
  align-items: center;
  box-sizing: border-box;
}

.header .tag {
  left: 32rpx;
  top: 0;
  background-image: url('//img.aimoge.com/FiEMBvAO0UXoH5hQ--0L8uSfXHDj');
  background-repeat: no-repeat;
  background-size: 48rpx 48rpx;
  background-position: center center;
  width: 48rpx;
  height: 48rpx;
}

.header .input {
  flex: 1;
}

.header .text {
  color: #666;
  font-size: 32rpx;
  height: 48rpx;
  line-height: 48rpx;
  background-color: #fff;
  border-radius: 12px;
  padding: 0 48rpx 0 32rpx;
  margin-left: 32rpx;
}

.search_btn {
  background-image: url('//img.aimoge.com/FrszcL0jvUD8rK8BqCUlFRN5ZgwM');
  background-position: 0 0;
  background-repeat: no-repeat;
  position: absolute;
  background-size: 80% 80%;
  width: 30rpx;
  height: 42rpx;
  right: 46rpx;
  top: 50%;
  margin-top: -16rpx;
  display: block;
}

/*内容部分*/

.content {
  padding-bottom: 156rpx;
  padding-left: 32px;
  background: #fff;
  
}

.item {
  padding: 32rpx;
  padding-left: 0;
  display: flex;
  -webkit-display: flex;
  color: #353535;
  position: relative;
  font-size: 32rpx;
  /*justify-content: space-around;*/
  border-bottom: 1rpx solid rgba(151,151,151,0.5);
}

.check_icon {
  position: absolute;
  width: 80rpx;
  height: 80rpx;
  top: -1rpx;
  right: 0;
  background-image: url('//img.aimoge.com/Fv3CE1ZdLfo-GWI8V1tI21BZF111');
  background-position: 0 0;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.item .right {
  margin-left: 70rpx;
}

.item .right view {
  height: 64rpx;
  line-height: 64rpx;
}

.item .left text {
  line-height: 70rpx;
  height: 70rpx;
  font-size: 14px;
  text-align: center;
}

.item .left .image {
  margin-top: 30rpx;
