.none:before {
  height: 0;
}

.nocheack {
  color: #ccc;
}

.footer {
  bottom: 0;
  left: 0;
  background: #fff;
  display: flex;
  width: 100%;
  height: 120rpx;
  align-items: center;
  margin-top: 28rpx;
  justify-content: space-between;
  padding-left: 36rpx;
  font-size: 14px;
  box-sizing: border-box;
}

.footer .agree {
  width: 36rpx;
  height: 36rpx;
  margin-right: 32rpx;
  background-image: url('//img.aimoge.com/FtScIduXUKVq__ZZB6DTWyffHZ3S');
  background-position: 0 0;
  background-repeat: no-repeat;
  background-size: 100%;
}

.footer .noagree {
  background-image: url('//img.aimoge.com/FrQrIw5WtrzVJKqQVxAUK58zema5');
  background-position: 0 0;
  background-repeat: no-repeat;
  background-size: 100%;
}

.footer .btn {
  width: 192rpx;
  height: 120rpx;
  color: #fff;
  background: #4285f4;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 16px;
}

.footer .disabled {
  background: #b2b2b2;
  color: #eaeaea;
  width: 192rpx;
  height: 120rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 16px;
}

/*弹出框样式*/

.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
}

.drawer_box {
  width: 600rpx;
  overflow: hidden;
  position: fixed;
  top: 50%;
  left: 50%;
  z-index: 1001;
  background: #fff;
  margin-top: -150rpx;
  margin-left: -300rpx;
  border-radius: 5px;
  text-align: center;
}

.drawer_title {
  padding-top: 20rpx;
  padding-bottom: 60rpx;
  font: 16px "microsoft yahei";
  text-align: center;
}

.drawer_icon {
  width: 106rpx;
  height: 106rpx;
  display: inline-block;
  margin-top: 40rpx;
}

/*公共样式*/

.blue {
  color: #4285f4;
}

.gray {
  color: #ccc;
}

.margin_t30 {
  margin-top: 30rpx;
}

.container imgae {
  width: 100%;
  height: 100%;
}
