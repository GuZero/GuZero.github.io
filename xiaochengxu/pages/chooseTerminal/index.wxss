page {
  background-color: #fff;
  height: 100%;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;
}

.item {
  background-color: #fff;
  margin-bottom: 10rpx;
  padding: 30rpx 0 30rpx 130rpx;
  font-size: 28rpx;
  color: #888;
}

.item:active {
  background-color: #fafafa;
}

.item image {
  width: 90rpx;
  height: 90rpx;
  top: 20rpx;
  left: 20rpx;
  border-radius: 100%;
}

.item .h3 {
  font-size: 32rpx;
  color: #353535;
  padding: 4rpx 0 10rpx 0;
}

.search {
  margin: 0;
  top: 0;
  z-index: 300;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  padding: 0 32rpx;
  font-size: 32rpx;
  color: #c3c3c3;
  border-bottom: 1rpx #eee solid;
  background-color: #4285f4;
  align-items: center;
  box-sizing: border-box;
}

.search .input {
  flex: 1;
}

.search input {
  color: #666;
  font-size: 32rpx;
  height: 48rpx;
  line-height: 48rpx;
  background-color: #fff;
  border-radius: 4px;
  padding: 0 64rpx 0 32rpx;
  margin: 0 32rpx;
}

.search .input .tag {
  right: 48rpx;
  top: 0;
  background-image: url('//img.aimoge.com/Fp5Fb_0YJWJASyYvqsDVGCrPXbvC');
  background-repeat: no-repeat;
  background-size: 32rpx 32rpx;
  background-position: center center;
  width: 48rpx;
  height: 48rpx;
  z-index: 300;
}

.search .search-btn {
  width: 92rpx;
  height: 48rpx;
  line-height: 48rpx;
  border: 1px solid #fff;
  border-radius: 4px;
  color: #fff;
  text-align: center;
}

.content {
  padding-top: 90rpx;
}

.content-title {
  height: 48rpx;
  line-height: 48rpx;
  padding: 0 32rpx;
  color: #888;
  font-size: 24rpx;
  background-color: #efeff4;
}

.content .terminal {
  color: #000;
  padding: 32rpx 160rpx 32rpx 32rpx;
  font-size: 36rpx;
}

.terminal .dist {
  right: 0;
  top: 0;
  padding: 32rpx;
  padding-left: 0;
  color: #888;
  font-size: 32rpx;
}

.nearby .item {
  margin-bottom: 24px;
  font-size: 16px;
  color: #4d4d4d;
}

.content .pos-loading {
  color: #000;
  padding: 0 160rpx 0 32rpx;
  height: 106rpx;
  line-height: 106rpx;
  font-size: 36rpx;
}

.content .pos-loading .repos {
  right: 32rpx;
  top: 0;
  height: 106rpx;
  line-height: 106rpx;
  padding-left: 56rpx;
  background-image: url('//img.aimoge.com/FuCPy6uTtcXC0CVP5n740M-ZIRps');
  background-repeat: no-repeat;
  background-size: 38rpx 38rpx;
  background-position: left center;
  font-size: 32rpx;
  color: #4285f4;
}
